{
  "name": "code-interview",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run transpile",
    "setup:fakeintegration": "cd ./fake-integration && npm install",
    "run:fakeintegration": "cd ./fake-integration && npm run dev",
    "start": "ts-node ./tdd-project/index.ts",
    "test": "jest --detectOpenHandles",
    "test:watch": "tsc -b ./tsconfig.project.test.json -- --watch & npm run test -- --watch",
    "clean": "rimraf ./build",
    "transpile": "tsc -p ./tsconfig.json",
    "watch-ts": "tsc -w",
    "format": "prettier \"**/*.{js,jsx,ts,md}\" --config ./.prettierrc",
    "format:fix": "npm run format -- --write",
    "lint": "eslint --ext .js,.jsx,.ts .",
    "lint:errors": "npm run lint -- --quiet",
    "lint:fix": "npm run lint -- --fix",
    "lint:debug": "DEBUG='eslint:*,eslintrc:*' eslint .",
    "lintformat:fix": "npm run format:fix && npm run lint:fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/q": "^1.5.5",
    "axios": "^1.7.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@jest/globals": "^29.6.4",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.202",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.0.3",
    "jest-extended": "^4.0.2",
    "jest-junit": "^14.0.1",
    "jest-when": "^3.5.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prettier-eslint": "^14.1.0",
    "rimraf": "^5.0.1",
    "supertest": "^6.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "yargs": "^17.6.2",
    "yargs-parser": "^21.1.1"
  }
}
